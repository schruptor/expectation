{
  "name": "schruptor/expectation",
  "description": "A class to validate assertions",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "schruptor",
      "email": "schruptor+expectation@mailbox.org"
    }
  ],
  "require": {
    "php": ">=7.2.0"
  },
  "autoload": {
    "psr-4": {
      "Schruptor\\Expectation\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Schruptor\\Expectation\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "pestphp/pest": "^0.2.3",
    "larapack/dd": "dev-master"
  }
}
